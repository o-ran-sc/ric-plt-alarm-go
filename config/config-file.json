{
    "local": {
        "host": ":8089"
    },
    "logger": {
        "level": 4
    },
    "db": {
        "namespaces": ["sdl", "rnib"]
    },
    "rmr": {
        "protPort": "tcp:4560",
        "maxSize": 1024,
        "numWorkers": 1
    },
    "controls": {
        "promAlertManager": {
            "address": "elfkp-prometheus-alertmanager:9093",
            "baseUrl": "/api/v2",
            "schemes": "http",
            "alertInterval": 30000
        },
        "maxActiveAlarms": 5000,
        "maxAlarmHistory": 20000
    }
}
